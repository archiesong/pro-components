  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "> 1%",
    "ie >= 11"
  ],
  "dependencies": {
    "@ctrl/tinycolor": "^3.5.0",
    "dayjs": "^1.10.5",
    "swr-vue": "^1.9.1"
  },
  "peerDependencies": {
    "vue": ">=3.0.0",
    "ant-design-vue": ">=4.0.0"
  },
  "publishConfig": {
