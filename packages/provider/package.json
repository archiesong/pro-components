{
  "name": "@ant-design-vue/pro-provider",
  "version": "1.0.5",
  "type": "module",
  "description": "@ant-design-vue/pro-provider",
  "keywords": [
    "antdv",
    "admin",
    "ant-design-vue",
    "ant-design-vue-pro"
  ],
  "homepage": "https://github.com/asuradaSong/pro-components/packages/provider#readme",
  "bugs": "https://github.com/asuradaSong/pro-components/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/asuradaSong/pro-components.git"
  },
  "license": "MIT",
  "sideEffects": false,
  "main": "lib/index.js",
  "module": "es/index.js",
